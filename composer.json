{
    "name": "jager/HTMLBuilder",
    "description": "HTML Builder API",
    "keywords": ["HTML", "Builder", "HTML Builder"],
    "support": {
        "issues": "https://github.com/martijnjager/HTMLBuilder/issues",
        "source": "https://github.com/martijnjager/HTMLBuilder"
    },
    "license": "MIT",
    "author": {
        "name": "Martijn Jager",
        "email": "martijnjager@live.nl"
    },
    "require": {
        "php": ">=5.6.4"
    },
    "require-dev":{
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "HTMLBuilder\\": "HTMLBuilder/src"
        }
    },
    "minimum-stability": "dev",
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
